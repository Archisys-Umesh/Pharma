{% extends "modalForm.twig" %}
{% block modalTitle %} {{form_name}} {% endblock %}
{% block modalBody %}    
    {{form | raw}}        	                                        
{% endblock %}
{% block script %}
<script type ='text/javascript'>
        
    $(document).ready(function(){                    
        $claims = [];
        $('.expChk').each(function($idx,$obj){
            
            if($($obj).is(":checked"))
            {
                $claims.push($($obj).attr("ExpId"));
            }
            
        });
        
        $("#expidmove").val($claims.join(","));
        
    });

</script>
{% endblock %}        
