        <script>
            
            function initAutocomplete()
            {
                
            }
            
            $loaded = false;
                                    
            $(document).ready(function(){                                
                initMain();                
            });
            
            function initMain()
            {
                if(!$loaded) {
                    hideLoader();

                    {% if toster_info %}
                            toastr["info"]("{{toster_info}}", "");
                    {% endif%}

                    {% block footerScript %}{% endblock %}
                    $loaded = true;
                }
            }
            
            setTimeout(function(){
                initMain();
            },10000);
  
            $(document).ready(function(){
                {% if firstLogin == true %}
                    dashboard_intro();
                {% endif %}
                {% if firstEmployee == true %}
                    employeeIntro();
                {% endif %}
                {% if firstExpenseMaster == true %}
                    expenseHeadIntro();
                {% endif %}
                {% if firstapproverIntro == true %}                    
                    approverIntro();
                {% endif %}
                {% if firstTripSingleIntro == true %}
                    tripSingleIntro();
                {% endif %}
                {% if firstAuditorIntro == true %}
                    auditorIntro();
                {% endif %}

                $("#fullScreenShow").hide();
                $("#fullScreen").click(function(){
                    $(".topbar-left").hide();
                    $("#left-sideMenu").hide();
                    $(".content-page").css("margin-left", "0px");
                    $("#fullScreen").hide();
                    $("#fullScreenShow").show();
                    localStorage.setItem('sidebarhide',true);
                });
                $("#fullScreenShow").click(function(){
                    $(".topbar-left").show();
                    $("#left-sideMenu").show();
                    $(".content-page").css("margin-left", "250px");
                    $("#fullScreen").show();
                    $("#fullScreenShow").hide();
                    localStorage.setItem('sidebarhide',false);
                });
            });
        </script>
